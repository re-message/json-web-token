{
    "name": "relmsg/json-web-token",
    "description": "A JSON Web Token (RFC 7519) implementation library",
    "type": "library",
    "license": "Apache License 2.0",
    "homepage": "https://dev.relmsg.ru/json-web-token",
    "authors": [
        {
            "name": "Oleg Kozlov",
            "email": "h1karo@relmsg.ru",
            "homepage": "https://relmsg.ru/h1karo",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-sodium": "*",
        "ext-mbstring": "*",
        
        "psr/event-dispatcher": "^1.0.0",
        "symfony/event-dispatcher": "^4.4.2",
        "webmozart/json": "^1.2.2",
        "monolog/monolog": "^2.0.2",
        "doctrine/collections": "^1.6.4",
        "doctrine/annotations": "^1.8.0",
        "paragonie/constant_time_encoding": "^2.3"
    },
    "require-dev": {
        "ext-redis": "*",
        "ext-memcache": "*",
        
        "laminas/laminas-math": "^3.2",
        
        "phpunit/phpunit": "^8.5.1",
        "symfony/phpunit-bridge": "^4.4.2",
        "symfony/stopwatch": "^4.4.2",
        
        "ramsey/uuid": "^3.9.2"
    },
    "suggest": {
        "ext-redis": "Allow to use redis token storage",
        "ext-memcache": "Allow to use memcache token storage",
        "ramsey/uuid": "Allow to use UUID identifier generators"
    },
    "autoload": {
        "psr-4": {
            "RM\\Security\\Jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RM\\Security\\Jwt\\Tests\\": "tests/"
        }
    }
}
